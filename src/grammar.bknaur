<program>    ::= <statement>*
<statement>  ::= <assignment> ";" |
                      <print> ";" |
<value>      ::= <expression> |
                     "Number" |
                 "Identifier" |
<assignment> ::= "Identifier" ":" <value>
<expression> ::= "Operator" <value> <value> 
<print>      ::= <expression> "="